{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "64robots/nova-fields": "^0.22.0",
        "bakerkretzmar/nova-settings-tool": "1.1.3",
        "benjacho/belongs-to-many-field": "^2.0",
        "chaseconey/nova-external-image": "^0.3.1",
        "comodolab/nova-help": "^0.5.0",
        "digital-creative/clickable-table-row": "^0.2.0",
        "digital-creative/conditional-container": "^1.4",
        "ebess/advanced-nova-media-library": "^3",
        "epartment/nova-dependency-container": "^1.3",
        "facebook/graph-sdk": "5.1.4",
        "guzzlehttp/guzzle": "^7.2",
        "jackabox/nova-duplicate-field": "^0.3.0",
        "laraning/nova-time-field": "^1.2",
        "laravel/framework": "^9.19",
        "laravel/nova": "*",
        "laravel/sanctum": "^3.0",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "laravelwebdev/responsive": "^1.0",
        "naoray/nova-json": "^2.0",
        "orlyapps/nova-belongsto-depend": "^3.0",
        "outl1ne/nova-simple-repeatable": "^1.4",
        "outl1ne/nova-sortable": "^2.4",
        "pbmedia/laravel-ffmpeg": "^8.1",
        "pdewit/nova-external-url": "^1.0",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-tags": "^4.3",
        "spatie/nova-tags-field": "^3",
        "superlatif/nova-tag-input": "3.0",
        "symfony/process": "^6.1",
        "whitecube/nova-flexible-content": "^0.2",
        "ziffmedia/nova-select-plus": "^1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Forkage\\": "forkage/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
